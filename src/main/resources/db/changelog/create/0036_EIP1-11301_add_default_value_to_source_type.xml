<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.1.xsd">

    <changeSet author="kelly.fragkogianni@softwire.com" id="0036_EIP1-11301_add_default_value_to_source_type" context="ddl">
        <addDefaultValue tableName="register_check" columnName="source_type" defaultValue="APPLICATIONS_API"/>
        <dropNotNullConstraint tableName="register_check" columnName="source_type" columnDataType="varchar(100)"/>

        <rollback>
            <dropDefaultValue tableName="register_check" columnName="source_type"/>
            <addNotNullConstraint tableName="register_check" columnName="source_type" columnDataType="varchar(100)"/>
        </rollback>
    </changeSet>

</databaseChangeLog>