<?xml version="1.0" encoding="UTF-8" ?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.1.xsd">
    <changeSet author="dan.wilkins@softwire.com" id="0040_EROPSPT-538_add_application_reference_to_register_check_table">
        <addColumn tableName="register_check">
            <column name="application_reference" type="varchar(10)" afterColumn="source_reference" />
        </addColumn>

        <rollback>
            <dropColumn tableName="register_check" columnName="application_reference" />
        </rollback>
    </changeSet>
</databaseChangeLog>